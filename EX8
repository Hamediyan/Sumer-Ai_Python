list1 = []

def Fib(n):

  if list1[n] != -1:
    return list1[n]
  if n == 0:
    list1[0] = 0
    return 0 #base case 1
  elif n == 1:
    list1[1] = 1;
    return 1  #base case 2
  else:
    list1[n] = Fib(n-1) + Fib(n-2);
    return list1[n];
def pfib(n):
    for i in range(n+1):
        list1.append(-1);
    Fib(n)
    print (list1)
pfib(8)   
